<!DOCTYPE html>
<html>
  <head>
    <title>Java</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <section>
    </section>

    <script>
    'use strict';

    function fizzbuzz(num) {
      if(num % 3 === 0 && num % 5 ===0) {
        return 'fizzbuzz';
      } else if(num % 3 === 0) {
        return 'fizz';
      } else if(num % 5 === 0) {
        return 'buzz';
      } else {
        return num;
      }
    }
    console.log(fizzbuzz(47590));
    </script>
    <%= yield %>
  </body>
</html>
